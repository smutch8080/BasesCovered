# Set MIME types for JavaScript files
/static/assets/*-*.js
  Content-Type: text/javascript

/static/assets/*.js
  Content-Type: text/javascript
  
/*.js
  Content-Type: text/javascript

/*.mjs
  Content-Type: text/javascript

# Set MIME types for CSS files
/static/assets/*-*.css
  Content-Type: text/css

/static/assets/*.css
  Content-Type: text/css

/*.css
  Content-Type: text/css 